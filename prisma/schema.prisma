generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model City {
  id        String   @id @default(cuid())
  name      String
  country   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([name, country])
}
